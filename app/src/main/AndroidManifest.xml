        android:theme="@style/Theme.Turnbasedgame_base">
        <activity android:name="mcm.edu.ph.MorphTech_turnbasedgame.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>